FUNCTION swap :
PARAM a
PARAM b
tem := a
b := a
a := b
WRITE a
WRITE tem
RETURN a
FUNCTION main :
DEC t11 20
x := &t11
i := #0
LABEL label1 :
IF i < #5 GOTO label2
GOTO label3
LABEL label2 :
t17 := x
t18 := i
t19 := #4 * t18
t20 := t17 + t19
READ t21
*t20 := t21
t15 := *t20
t22 := i
t23 := #0
IF t22 > t23 GOTO label4
GOTO label5
LABEL label4 :
t27 := i - #1
t30 := #4 * t27
t31 := x + t30
t35 := #4 * i
t36 := x + t35
ARG *t36
ARG *t31
t24 := CALL swap
LABEL label5 :
i := i + #1
GOTO label1
LABEL label3 :
RETURN #0
